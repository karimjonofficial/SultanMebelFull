<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->
    <SolidColorBrush x:Key="Button.Static.Background" Color="#FF40A7E3"/>
    <SolidColorBrush x:Key="Button.Static.Border" Color="#FF3684D2"/>
    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FF57B0E3"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF51A3D2"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FF3BA5E3"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF3699D2"/>
    <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
</ResourceDictionary>