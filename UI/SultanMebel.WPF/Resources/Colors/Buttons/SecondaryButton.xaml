<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Colors-->

    <SolidColorBrush x:Key="Button.Static.Background" Color="#FFE9E9E9"/>
    <SolidColorBrush x:Key="Button.Static.Border" Color="#FFADB2B5"/>
    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFF5F5F5"/>
    <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FFBEC4C7"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFE2E8EB"/>
    <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF87B4EB"/>
    <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
    <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
    <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>

</ResourceDictionary>